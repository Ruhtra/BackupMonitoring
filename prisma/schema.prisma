// prisma/schema.prisma

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Reg {
  id           String   @id
  dbName       String
  statusBackup String
  statusSend   String
  createdAt    DateTime

  @@map("reg")
}
